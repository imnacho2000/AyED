package tests;

import tp02.ejercicio2.Alumno;
import tp02.ejercicio2.ListaEnlazadaGenerica;

public class TestListaEnlazadaGenerica {
	public static void main(String [] args) {
		Alumno alumnoUno = new Alumno("Miqueas","Brambilla","Micky@gmail.com","esta",5);
		Alumno alumnoDos = new Alumno("Ignacio","Carrera","Nachis@gmail.com","esta",3);
		Alumno alumnoTres = new Alumno("Dante","Fernandez","DanteFdz@gmail.com","esta",6);
		Alumno alumnoCuatro = new Alumno("Julian","Order","july@gmail.com","esta",3);
		ListaEnlazadaGenerica<Alumno> lista = new ListaEnlazadaGenerica<Alumno> ();
		lista.agregarInicio(alumnoUno);
		lista.agregarInicio(alumnoDos);
		lista.agregarInicio(alumnoTres);
		lista.agregarInicio(alumnoCuatro);
		for(int n = 0; n <= lista.tamanio(); n++) {	
			System.out.print("Alumno: " + n);
			System.out.println( ", " + lista.elemento(n).tusDatos());
		}
	}
}
