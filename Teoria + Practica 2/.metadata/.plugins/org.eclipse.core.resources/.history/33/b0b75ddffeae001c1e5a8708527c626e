package tests;



import tp02.ejercicio1.*;




public class TestListaDeEnterosConArreglos {
	public static void main (String []args){
		ListaDeEnterosConArreglos lista = new ListaDeEnterosConArreglos();
		lista.comenzar();
		Integer secuencia;
		for (int n = 0; n < 10; n++) {
			secuencia = n*2;
			lista.agregarFinal(secuencia);
		}
		System.out.println("Imprimiendo lista: ");
		for (int n = 1; n < 10; n++) {
			System.out.print(lista.elemento(n) + " ");
		}
		System.out.println(" ");
		System.out.println("Orden inverso: ");
		imprimirInverso(lista);
	}
	public String imprimirInverso(ListaDeEnterosConArreglos lista) {
		imprimirInverso(lista.proximo());
		lista = lista.tamanio() - 1;
		return lista.elemento(lista.tamanio()).toString();
	}
}
